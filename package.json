{
  "name": "next13-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:css": "unocss -c uno.config.ts -o ./src/shared/styles/app.css -w ./src/**/*.tsx",
    "dev:next": "next dev",
    "dev": "run-p dev:*",
    "css": "unocss -c uno.config.ts -o ./src/shared/styles/app.css ./src/**/*.tsx -m",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:tsc": "tsc -p . --noEmit",
    "lint:staged": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh ./.husky/pre-commit",
      "pre-push": "sh ./.husky/pre-push",
      "commit-msg": "sh ./.husky/commit-msg"
    }
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@next/font": "^13.4.12",
    "axios": "^1.4.0",
    "next": "13.4.5",
    "next-auth": "^4.22.1",
    "next13-boilerplate": "link:",
    "qs": "^6.11.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.9.0",
    "react-select": "^5.7.3",
    "react-use": "^17.4.0",
    "sass": "^1.60.0",
    "swiper": "^10.1.0",
    "swr": "^2.1.5",
    "tailwind-merge": "^1.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@unocss/cli": "^0.52.3",
    "@unocss/preset-uno": "^0.52.3",
    "@unocss/preset-web-fonts": "^0.52.7",
    "@unocss/preset-wind": "^0.52.3",
    "@unocss/reset": "^0.52.3",
    "@unocss/transformer-compile-class": "^0.52.3",
    "@unocss/webpack": "^0.52.3",
    "autoprefixer": "10.4.14",
    "clsx": "^1.2.1",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.3",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "typescript": "5.0.4",
    "unocss": "^0.52.3"
  },
  "browser": {
    "buffer": false
  }
}
